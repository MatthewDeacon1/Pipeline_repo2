+import subprocess
+
+def get_unversioned_files():
+    """Retrieve a list of unversioned files using Git."""
+    try:
+        result = subprocess.run(
+            ["git", "status", "--porcelain"],
+            capture_output=True,
+            text=True,
+            check=True
+        )
+        # Filter for untracked files marked with '??'
+        unversioned_files = [
+            line[3:] for line in result.stdout.splitlines() if line.startswith("??")
+        ]
+        return unversioned_files
+    except subprocess.CalledProcessError as e:
+        print("Error while retrieving unversioned files:", e.stderr)
+        return []
+
+def commit_file(file_path):
+    """Commit an individual file to the repository."""
+    try:
+        # Add the file to the staging area
+        subprocess.run(["git", "add", file_path], check=True)
+        # Commit the file with a message
+        commit_message = f"Add {file_path}"
+        subprocess.run(["git", "commit", "-m", commit_message], check=True)
+        print(f"Committed: {file_path}")
+    except subprocess.CalledProcessError as e:
+        print(f"Error committing {file_path}: {e.stderr}")
+
+def main():
+    unversioned_files = get_unversioned_files()
+    if not unversioned_files:
+        print("No unversioned files found.")
+        return
+
+    print(f"Found {len(unversioned_files)} unversioned file(s).")
+    for file_path in unversioned_files:
+        commit_file(file_path)
+
+if __name__ == "__main__":
+    main()
